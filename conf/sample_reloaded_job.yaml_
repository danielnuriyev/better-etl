{
  "t": time.time()
}
name: test_job
schedule: 0 0 * * *
retries:
  max: 2
  delay: 1
  backoff: exponential
  lookback_minutes: 5
  notifier:
    package:  better_etl.notifiers.notifier
    class:    Notifier
    config:
      test: test
ops:
  - name: get_secret
    package:  better_etl.ops
    class:    AWSSecretsManager
    method:   get_secret
    config:
      secret_name: ...
  - name: get_batches
    after:
      - get_secret
    package:  better_etl.ops
    class:    MySQL
    method:   get_batches
    config:
      host:     ...
      database: ...
      table:    ...
      batch:    1000
      batches:  10
  - name: store_batches
    after:
      - get_batches
    package:  better_etl.ops
    class:    AWSS3
    method:   store
    config:
      bucket: ...
      path:   tables/.../$t
      condition: >
        batch["data"].size > 0 and
        batch["data"].size < 100000
